{
  "name": "Sigue Mis Pasos",
  "description": "Sigue Mis Pasos",
  "keywords": ["framework", "laravel", "lumen", "angularjs", "bower"],
  "license": "MIT",
  "type": "project",
  "require": {
    "laravel/lumen-framework": "5.1.*",
    "vlucas/phpdotenv": "~1.0",
    "firebase/php-jwt" : "*",
    "guzzlehttp/guzzle": "*"
  },
  "require-dev": {
      "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
        "App\\": "backend/app/"
    },
    "classmap": [
        "backend/database/"
    ]
  },
  "autoload-dev": {
    "classmap": [
        "backend/tests/"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}